                rpc:
                  port: 33145
              logging:
                logLevel: trace
                usageStats:
                  enabled: false
              resources:
                cpu:
                  cores: 1
                memory:
                  container:
                    max: 2Gi
                    min: 2Gi
              statefulset:
                replicas: 3
              storage:
                persistentVolume:
                  enabled: true
                  size: 100Gi
              tls:
                certs:
                  kafka-internal-0:
                    caEnabled: true
                enabled: true
          ---
          apiVersion: cert-manager.io/v1
          kind: Certificate
          metadata:
            name: cluster-tls-user-client
          spec:
            emailAddresses:
              - test@domain.dom
            duration: 43800h0m0s
            issuerRef:
              group: cert-manager.io
