                    enabled: true
                    requireClientAuth: true
              adminApi:
              - port: 9644
              pandaproxyApi:
              - port: 8082
              developerMode: true
          
          EOF
          $KUBECTL -n default delete pvc --all
      volumeMounts:
      - mountPath: /var/run/secrets/kubernetes.io/serviceaccount
        name: kube-api-access
        readOnly: true

  restartPolicy: Never
  volumes:
  - name: kube-api-access
