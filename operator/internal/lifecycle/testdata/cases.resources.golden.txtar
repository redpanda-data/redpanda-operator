      LISTENER="{\"address\":\"${SERVICE_NAME}.basic-test.basic-test.svc.cluster.local.\",\"name\":\"internal\",\"port\":8082}"
      rpk redpanda config --config "$CONFIG" set pandaproxy.advertised_pandaproxy_api[0] "$LISTENER"

